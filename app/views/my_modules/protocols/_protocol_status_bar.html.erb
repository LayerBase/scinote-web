<div class="panel panel-default panel-protocol-status popover_v2"
data-popoverlink="<%= I18n.t('popover_test.link.protocol.linking') %>"
data-popovercontent="<%= I18n.t('popover_test.text.protocol.linking') %>">
  <div class="panel-body">
    <% if @protocol.linked? %>
      <%= protocol_status_href(@protocol) %>
    <% else %>
      <%= t("my_modules.protocols.protocol_status_bar.unlinked") %>
    <% end %>
    &nbsp;
    <%= render partial: "my_modules/protocols/protocol_status_bar_buttons.html.erb" %>
  </div>
</div>
<%= javascript_include_tag("my_modules/protocols/protocol_status_bar") %>
